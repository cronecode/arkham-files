<%= form_for([@campaign, @scenario]) do |f| %>
    <%= render 'shared/error_messages', :target => @scenario %>
    <div class="form-group">
      <%= f.label :name %>:
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <%= f.submit "Add scenario", class: "btn btn-primary" %>
<% end %>